{
    "globals": {
        "setTimeout": false,
        "requirejs": false,
        "require": false,
        "define": false,
        "jasmine": false,
        "spyOn": false,
        "it": false,
        "describe": false,
        "expect": false,
        "beforeEach": false,
        "afterEach": false,
        "waits": false,
        "waitsFor": false,
        "runs": false,
        "window": false
    },
    // Enforcing
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "freeze": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    // Limits
    "maxparams": false,
    "maxdepth": false,
    "maxstatements": 16,
    "maxcomplexity": false,
    "maxlen": 120,
    // Relaxing
    "es5": true,
    "esnext": false
}
